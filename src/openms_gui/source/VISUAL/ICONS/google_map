    <script
      defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDpwXKFsIKcZFoM03TJsE4AdIbvEohzUF4&callback=initMap"
    ></script>
    <script defer>
      function initMap() {
        let mapProp = {
          center: new google.maps.LatLng(51.508742, -0.12085),
          panControl: true,
          zoom: 5,
        };
        let map = new google.maps.Map(document.getElementById("map"), mapProp);
      }
    </script>
  </head>
  <body>
    <div id="map" style="width: 100%; height: 400px"></div>
  </body>
