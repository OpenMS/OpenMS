# The java package name of the plugin
# In maven called the artifactId
pluginPackage=@ID@

# The human-readable name of the plugin
pluginName=@NAME@

# The version of the plugin
pluginVersion=@VERSION@

# The path for the nodes in the Node repository of KNIME
# community = starting from KNIMEs Community Nodes "folder"
# The plugin name will be used as the name for the added subfolder.
# Then the tools names as noted in the Tool description (CTD).
nodeRepositoyRoot=community

executor=com.genericworkflownodes.knime.execution.impl.LocalToolExecutor
commandGenerator=com.genericworkflownodes.knime.execution.impl.OpenMSCommandGenerator