# The "provider" of this feature.
groupId=de.openms

# The java package name of the plugin
# In maven called the artifactId
artifactId=de.openms.update

# All category IDs on this update site. Semi-colon separated.
categories=openms;openmsthirdparty

# For each category ID, you can optionally add a "_Label" and a longer "_Description".
openms_Description=Core OpenMS library and tools/nodes
openms_Label=OpenMS Nodes
openmsthirdparty_Label=OpenMS Thirdparty Nodes
openmsthirdparty_Description=Thirdparty tools/nodes that can interface with OpenMS

